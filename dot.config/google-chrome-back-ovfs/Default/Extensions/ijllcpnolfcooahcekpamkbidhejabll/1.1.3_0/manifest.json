{
   "author": "Kenshin Wang \u003Ckenshin@ksria.com>",
   "background": {
      "scripts": [ "/bundle/common.js", "/bundle/background.js" ]
   },
   "content_scripts": [ {
      "exclude_matches": [ "http://localhost/*", "https://simpread.herokuapp.com/view/*" ],
      "js": [ "/bundle/common.js", "/bundle/vendors.js", "/bundle/contentscripts.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*/*.txt", "file:///*/*.md" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_extension_desc__",
   "homepage_url": "http://ksria.com/simpread",
   "icons": {
      "128": "assets/images/icon128.png",
      "16": "assets/images/icon16.png",
      "48": "assets/images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extension_name__",
   "offline_enabled": true,
   "options_page": "options/options.html",
   "page_action": {
      "default_icon": {
         "16": "assets/images/icon16.png"
      }
   },
   "permissions": [ "contextMenus", "tabs", "storage", "tts" ],
   "short_name": "SimpRead",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.3",
   "web_accessible_resources": [ "/assets/images/*", "website_list.json" ]
}
