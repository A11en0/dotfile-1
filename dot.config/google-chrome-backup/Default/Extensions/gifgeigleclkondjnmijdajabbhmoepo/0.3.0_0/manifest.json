{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "description": "Access gateway.ipfs.io urls the way they were meant: from your locally running ipfs daemon",
   "icons": {
      "128": "ipfs-logo-128-white.png",
      "16": "ipfs-logo-128-white.png",
      "48": "ipfs-logo-128-white.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "IPFS Gateway Redirect",
   "optional_permissions": [ "file://*/*" ],
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "page_action": {
      "default_icon": {
         "19": "ipfs-logo-128-white.png",
         "38": "ipfs-logo-128-white.png"
      },
      "default_title": "Copy as IPFS link"
   },
   "permissions": [ "clipboardWrite", "contextMenus", "declarativeContent", "storage", "webRequest", "webRequestBlocking", "http://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3.0"
}
