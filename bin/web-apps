#!/bin/bash

BROWSER="/usr/bin/x-www-browser"

baidupan(){
    bash -c "$BROWSER https://pan.baidu.com" &>/dev/null &
}

calibre1(){
	# jkxjxxz:swfcxs
	bash -c "$BROWSER http://cs3/tech" 2>/dev/null &
}
calibre2(){
	# calibre:1984
	bash -c "$BROWSER http://pub/library" 2>/dev/null &
}
calibre3(){
	# sensitive:1989
	bash -c "$BROWSER http://pub/sensitive" 2>/dev/null &
}

dingding(){
	bash -c "$BROWSER --new-window --app=https://im.dingtalk.com" 2>/dev/null &
}

ff(){
	firefox -url 'data:text/html;charset=utf-8,<!DOCTYPE html><html><body><script>window.open("https://gmail.com", "_blank","menubar=no,location=no,toolbar=no")<%2Fscript><%2Fbody><%2Fhtml>'
}

gcal(){
  bash -c "$BROWSER --new-window --app=https://www.google.com/calendar/render" 2>/dev/null &
}

gcontacts(){
	bash -c "$BROWSER https://contacts.google.com" 2>/dev/null &
}

gdrawing(){
  bash -c "$BROWSER https://docs.google.com/drawings/create?usp=apps_start&hl=en" 2>/dev/null &
}

gdrive(){
  bash -c "$BROWSER https://drive.google.com/drive/#my-drive" 2>/dev/null &
}

github(){
	bash -c "$BROWSER https://github.com" 2>/dev/null &
}

gkeep(){
  bash -c "$BROWSER --new-window --app=https://keep.google.com" 2>/dev/null &
}

gmail(){
  bash -c "$BROWSER --new-window --app=https://inbox.google.com" 2>/dev/null &
}

gmailoffline(){
  bash -c "$BROWSER https://mail.google.com/mail/mu/?mui=ca" 2>/dev/null &
}

gmaps(){
	QUERY=""
	for WORD in $@; do
		QUERY="$QUERY%20$WORD"
	done
	bash -c "$BROWSER --new-window --app=https://www.google.com/maps/search/$QUERY" 2>/dev/null &
}

google(){
  QUERY=""
  echo "Googling: $@"
  for WORD in $@; do
    QUERY="$QUERY%20$WORD"
  done
  bash -c "$BROWSER http://www.google.com/search?q=$QUERY" 2>/dev/null &
}

